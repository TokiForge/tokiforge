{
  "name": "@tokiforge/storybook",
  "version": "1.2.0",
  "description": "Storybook addon for TokiForge design tokens",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "keywords": [
    "storybook",
    "addon",
    "design-tokens",
    "tokiforge",
    "theming"
  ],
  "peerDependencies": {
    "@storybook/addons": "^7.0.0 || ^8.0.0",
    "@storybook/components": "^7.0.0 || ^8.0.0",
    "@storybook/api": "^7.0.0 || ^8.0.0",
    "react": "^18.0.0"
  },
  "dependencies": {
    "@tokiforge/core": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "author": "TokiForge Community",
  "license": "AGPL-3.0"
}

